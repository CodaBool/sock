{
  "agent": {
    "metrics_collection_interval": 600,
    "logfile": "/opt/aws/server.log"
  },
  "metrics": {
    "namespace": "Sock",
    "metrics_collected": {
      "disk": {
        "resources": ["/"],
        "measurement": ["used_percent"]
      },
      "mem": { "measurement": ["used_percent"] },
      "net": { "measurement": ["err_out"] },
      "netstat": { "measurement": ["tcp_established"] },
      "processes": { "measurement": ["total"] }
    }
  },
  "logs": {
    "log_stream_name": "sock",
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/server.log",
            "log_group_name": "/home/ec2-user/slap.log",
            "retention_in_days": 30
          },
          {
            "file_path": "/var/log/server.log",
            "log_group_name": "/home/ec2-user/typer.log",
            "retention_in_days": 30
          }
        ]
      }
    }
  }
}